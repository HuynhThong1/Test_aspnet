@model IEnumerable<FGW_Management.Models.Submission>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubmissionDue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubmissionDeadline_1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubmissionDeadline_2)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubmissionDue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubmissionDeadline_1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubmissionDeadline_2)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
